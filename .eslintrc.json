{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "plugins": ["prettier"],
  "rules": {
    "linebreak-style": ["error", "unix"],
    "prettier/prettier": ["error"],
    "no-undef": 0
  }
}
